
{
	"authors": ["HipremeEngine"],
	"description" : "Hipreme Engine test scene",
	"license": "proprietary",
	"targetName" : "test",
	"name" : "test",
	"sourcePaths"  : ["source"],
	"dependencies": 
	{
		"hipengine_api": {"path": "G:\\HipremeEngine/api"},
		"util": {"path": "G:\\HipremeEngine/modules/util"},
		"timer": {"path": "G:\\HipremeEngine/modules/timer"},
		"tween": {"path": "G:\\HipremeEngine/modules/tween"},
		"math": {"path": "G:\\HipremeEngine/modules/math"}
	},
	"stringImportPaths": [
		"."
	],
	"preBuildCommands": [
		"rdmd $HIPREME_ENGINE/tools/build/getmodules.d source/ scriptmodules.txt"
	],
	"dflags-ldc": [
		"-link-defaultlib-shared=false"
	],
	"configurations": 
	[
		{
			"name" : "script",
			"targetType": "dynamicLibrary",
			"versions": ["Script"],
			"lflags-windows": [
				"/WX"
			]
		},
		{
			"name": "run",
			"targetType": "dynamicLibrary",
			"versions": ["Script"],
			"lflags-windows": [
				"/WX"
			],
			"postGenerateCommands-windows": ["cd /d G:\\HipremeEngine && dub -c script -- G:\\HipremeEngine\\projects\\test"],
			"postGenerateCommands-linux": ["cd G:\\HipremeEngine && dub -c script -- G:\\HipremeEngine\\projects\\test"]
		}
	],
	"versions" : [
		"HipMathAPI",
		"HipremeAudio"
	]
}
	