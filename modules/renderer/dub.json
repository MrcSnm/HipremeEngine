{
	"authors": [
		"Hipreme"
	],
	"buildOptions": [
		"debugMode",
		"debugInfo",
		"debugInfoC"
	],
	"sourcePaths": [
		"source"
	],
	"lflags-windows": [
		"-Llib/win64"
	],
	"dependencies": {
		"hipengine_api:interfaces": {"path": "../../api"},
		"data" : {"path": "../data"},
		"image" : {"path": "../image"},
		"config": {"path": "../config"},
		"error": {"path": "../error"},
		"math": {"path": "../math"},
		"windowing" : {"path" : "../windowing"}
	},
	"copyright": "Copyright Â© 2021, Hipreme",
	"configurations": [
		{
			"name": "default",
			"dependencies": {
				"directx-d": {"path": "../../dependencies/directx-d", "optional": true, "default": true},
				"bindbc-opengl": {"version": "~>1.1.0", "optional": true, "default": true}
			},
			"versions": [
				"HipGL3",
				"OpenGL",
				"GL_45",
				"GL_ARB",
				"HipGLUseVertexArray"
			]
		},
		{
			"name": "dxonly",
			"dependencies": {"directx-d": {"path": "../../dependencies/directx-d"}}
		},
		{
			"name": "nodx",
			"dependencies": {
				"bindbc-opengl": "~>1.1.0"
			},
			"versions": [
				"HipGL3",
				"OpenGL",
				"GL_45",
				"GL_ARB",
				"HipGLUseVertexArray"
			]
		},
		{
			"name": "uwp",
			"dependencies": {
				"directx-d": {"path": "../../dependencies/directx-d"},
				"bind" : {"path": "../bind", "version": "*"}
			},
			"subConfigurations": {
				"image": "arsd"
			}
		},
		{
			"name": "appleos",
			"stringImportPaths": [
				"."
			],
			"dependencies": {
				"d-metal-binding": {"path": "../../dependencies/metal"}
			}
		},
		{
			"name": "wasm",
			"dependencies": {
				"gles" : {"path": "../../dependencies/gles"}
			},
			"subConfigurations": {
				"windowing": "wasm",
				"image": "wasm"
			},
			"versions": ["OpenGL", "GLES20"]
		},
		{
			"name": "psvita",
			"subConfigurations": {
				"windowing": "psvita",
				"image": "gamut"
			},
			"versions": ["OpenGL", "GLES20"],
			"dependencies": {"gles" : {"path": "../../dependencies/gles"}}
		},
		{
			"name": "android",
			"dependencies": {
				"gles" : {"path": "../../dependencies/gles", "version": "*"}
			},
			"subConfigurations": {
				"windowing": "android"
			},
			"versions": [
				"OpenGL", "GLES32", "HipGLUseVertexArray"
			]
		}
	],
	"description": "HipremeEngine renderer module",
	"license": "proprietary",
	"name": "renderer"
}