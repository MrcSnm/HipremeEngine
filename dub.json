		},
		"bind": {
			"path": "./modules/bind",
			"version": ">=0.0.0"
		},
		"bindbc-cimgui": "~>1.84.1",
		"bindbc-sdl": "~>1.0.1",
		"config": {
			"path": "./modules/config",
			"version": ">=0.0.0"
		},
		"console": {
