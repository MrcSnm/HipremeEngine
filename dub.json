{
	"authors": [
		"Hipreme"
	],
	"buildOptions": [
		"debugMode",
		"debugInfo",
		"debugInfoC"
	],
	"configurations": [
		{
			"name": "desktop",
			"targetPath": "bin/desktop",
			"targetType": "executable",
			"versions": [
				"Desktop"
			]
		},
		{
			"name": "uwp",
			"targetPath": "bin/uwp/",
			"targetType": "dynamicLibrary",
			"versions": [
				"dll",
				"UWP"
			],
			"postBuildCommands": [
				"cd .\\build && rdmd insertresourceuwp.d .\\uwp\\HipremeEngine\\HipremeEngine ..\\assets"
			]
		},
		{
			"name": "android",
			"dflags": [
				"-g"
			],
			"libs": [
				"log",
				"android",
				"OpenSLES",
				"EGL",
				"GLESv3"
			],
			"postBuildCommands": [
				"copy /B .\\bin\\android\\libhipreme_engine.so .\\build\\android\\project\\app\\src\\main\\jniLibs\\arm64-v8a\\libhipreme_engine.so",
				"cd .\\build && rdmd copyresources.d ..\\assets .\\android\\project\\app\\src\\main\\assets",
				"cd .\\build\\android\\project\\ && .\\gradlew.bat :app:assembleDebug && adb install -r .\\app\\build\\outputs\\apk\\debug\\app-debug.apk",
				"adb shell monkey -p com.hipremeengine.app 1",
				"adb logcat -b all -v color com.hipremengine.app:D | findstr HipremeEngine"
			],
			"targetPath": "bin/android/",
			"targetType": "dynamicLibrary",
			"versions": [
				"dll",
				"GLES30"
			]
		}
	],
	"copyright": "Copyright Â© 2018 - 2021, Hipreme",
	"dependencies": {
		"arsd-official:dom": "~>10.2.0",
		"bindbc-cimgui": "~>1.84.1",
		"bindbc-sdl": "~>1.0.1",
		"hipengine_api" : {"path": "./api"},
		"bind"		: {"path" : "./modules/bind"},
		"config"	: {"path" : "./modules/config"},
		"console"	: {"path" : "./modules/console"},
		"data"		: {"path" : "./modules/data"},
		"debugging"	: {"path" : "./modules/debugging"},
		"error"		: {"path" : "./modules/error"},
		"imgui"		: {"path" : "./modules/imgui"},
		"jni"		: {"path" : "./modules/jni"},
		"math"		: {"path" : "./modules/math"},
		"util"		: {"path" : "./modules/util"},
		"renderer"  : {"path" : "./modules/renderer"},
		"audio" 	: {"path" : "./modules/audio"}
	},
	"subConfigurations": {
		"hipengine_api" : "hipengine"
	},
	"description": "Open-Source D Game Engine",
	"dflags": [
		"-J=."
	],
	"importPaths-ldc" :
	[
		"C:/ldc2/import"
	],
	"libs-windows": [
		"ole32",
		"user32",
		"dxgi",
		"d3d11",
		"d3dcompiler"
	],
	"license": "proprietary",
	"name": "hipreme_engine",
	"sourcePaths": [
		"source"
	],
	"versions": [
		"HIPREME_DEBUG",
		"HipremeEngineDef",
		"SDL_208",
		"BindSDL_Mixer",
		"BindSDL_TTF",
		"BindSDL_Image",
		"GL_45",
		"GL_ARB",
		"AL_EFX"
	]
}