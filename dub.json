			"version": ">=0.0.0"
		},
		"debugging": {
			"path": "./modules/debugging",
			"version": ">=0.0.0"
		},
		"error": {
			"path": "./modules/error",
			"version": ">=0.0.0"
		},
		"hipengine_api": {
			"path": "./api",
			"version": ">=0.0.0"
		},
		"imgui": {
			"path": "./modules/imgui",
			"version": ">=0.0.0"
		},
		"jni": {
			"path": "./modules/jni",
